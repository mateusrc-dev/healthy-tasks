{
    "builds": [
      {
        "src": "next.config.js",
        "use": "@now/next"
      }
    ],
    "version": 2,
    "env": {
      "NEXT_PUBLIC_VAR": "public var",
      "ANOTHER_VAR": "another var"
    },
    "functions": {
        "api/**/*.js": {
          "includeFiles": ["prisma/schema.prisma"],
          "build": {
            "env": {
              "NODE_ENV": "production"
            },
            "command": "npm run build && npx prisma generate"
          }
        }
      }
  }